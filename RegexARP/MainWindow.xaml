<Window x:Name="txtId" x:Class="RegexARP.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RegexARP"
        mc:Ignorable="d"
        Title="MainWindow" Height="751.587" Width="817.794">
    <Grid Margin="0,0,-48,-6">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="340*"/>
            <ColumnDefinition Width="89*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="textInform" HorizontalAlignment="Left" Height="723" TextWrapping="Wrap" VerticalAlignment="Top" Width="677" SpellCheck.IsEnabled="True" AcceptsReturn="True" VerticalScrollBarVisibility="Visible" Background="#FFF9F9F0" RenderTransformOrigin="0.5,0.5">
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.144"/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>
        <Button x:Name="btbStart" Content="Start" Margin="0,-3,50,10" Click="Button_Click" Grid.Column="1"/>

    </Grid>
</Window>
